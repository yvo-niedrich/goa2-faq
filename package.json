{
  "name": "goa2-faq",
  "version": "6.3.1",
  "homepage": "https://yvo-niedrich.github.io/goa2-faq",
  "repository": {
    "type": "git",
    "url": "https://github.com/yvo-niedrich/goa2-faq"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build-data && npm run build-app",
    "build-data": "tsx --tsconfig tsconfig.build.json build/build.ts",
    "build-app": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "nightwatch tests/e2e/*",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^13.3.0",
    "marked": "^15.0.12",
    "pinia": "^3.0.1",
    "vite-plugin-ejs": "^1.7.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0",
    "vue3-carousel": "^0.16.0"
  },
  "devDependencies": {
    "@nightwatch/vue": "^3.1.2",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "chromedriver": "^135.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "geckodriver": "^5.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "nightwatch": "^3.12.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "sass-embedded": "^1.89.0",
    "tsx": "^4.19.4",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-nightwatch": "^0.4.6",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
